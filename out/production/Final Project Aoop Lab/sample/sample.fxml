<?import javafx.scene.text.Font?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <AnchorPane prefHeight="425.0" prefWidth="489.0">
         <children>
            <TextField fx:id="tf1" layoutX="120.0" layoutY="33.0" prefHeight="43.0" prefWidth="345.0" />
            <TextArea fx:id="ta1" editable="false" layoutX="120.0" layoutY="112.0" prefHeight="166.0" prefWidth="345.0">
               <font>
                  <Font size="18.0" />
               </font></TextArea>
            <Button layoutX="7.0" layoutY="177.0" mnemonicParsing="false" onAction="#on" prefHeight="36.0" prefWidth="96.0" text="Information" />
            <Button layoutX="33.0" layoutY="327.0" mnemonicParsing="false" onAction="#next" prefHeight="61.0" prefWidth="123.0" text="Vote">
               <font>
                  <Font size="27.0" />
               </font></Button>
            <Label alignment="CENTER" layoutX="7.0" layoutY="37.0" prefHeight="35.0" prefWidth="96.0" text="ID">
               <font>
                  <Font size="25.0" />
               </font>
            </Label>
            <Button layoutX="174.0" layoutY="327.0" mnemonicParsing="false" onAction="#result" prefHeight="61.0" prefWidth="141.0" text="Result">
               <font>
                  <Font size="27.0" />
               </font>
            </Button>
            <Button layoutX="338.0" layoutY="327.0" mnemonicParsing="false" onAction="#bot" prefHeight="61.0" prefWidth="132.0" text="Help!">
               <font>
                  <Font size="27.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
   </children>
</GridPane>
